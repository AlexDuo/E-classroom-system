	http://www.springframework.org/schema/tx
	http://www.springframework.org/schema/tx/spring-tx-2.5.xsd"
	default-autowire="byName" default-merge="true">

	<bean id="userService" class="Reservation.service.impl.UserServiceImpl"></bean>
	
	<bean id="productService" class="Reservation.service.impl.ProductServiceImpl"></bean>
		
